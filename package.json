{
  "name": "cloud-secrets",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "start": "yarn workspace @secrets/cloud start",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prisma": "yarn workspace @secrets/cloud prisma",
    "prisma:studio": "yarn prisma studio",
    "cloud": "yarn workspace @secrets/cloud"
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "^2.7.1",
    "turbo": "^1.4.3"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@3.2.3"
}
