{
  "name": "@envtree/cli",
  "description": "Node based CLI tool to download your secret configurations as .env or .json files from Env Tree",
  "author": {
    "name": "Max Rose",
    "url": "https://www.max-rose.com/"
  },
  "maintainers": [
    "Max Rose (https://www.max-rose.com/)"
  ],
  "version": "2.0.0",
  "exports": "./dist/index.js",
  "bin": {
    "envtree": "./dist/index.js"
  },
  "type": "module",
  "license": "GNU GPL V3",
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "secrets",
    "cloud",
    "download",
    "cli",
    "env",
    "tree",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/maxs-rose/env-tree",
    "directory": "apps/cli"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --clean --sourcemap --minify --metafile",
    "dev": "tsup src/index.ts --format esm --watch --clean",
    "test": "dist/index.js",
    "lint": "eslint src/ --fix"
  },
  "dependencies": {
    "chalk": "^5.0.1",
    "commander": "^9.4.0",
    "inquirer": "^9.1.1",
    "node-fetch": "^3.2.10",
    "open": "^8.4.0",
    "ora": "^6.1.2"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.1",
    "@types/node": "^18.7.18",
    "eslint": "8.22.0",
    "eslint-config-custom": "*",
    "ts-node": "^10.9.1",
    "tsconfig": "*",
    "tsup": "^6.2.3",
    "typescript": "4.8.3"
  }
}
